<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!-- Permissions for internet Connection -->

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.RVNT_Front"
        tools:targetApi="31">
        <activity
            android:name=".DetailEventActivity"
            android:exported="true">
<!--        <intent-filter>
                <action android:name="android.intent.action.DETAILEVENTACTIVITY" />

               <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter> -->          
        </activity>

        <activity
            android:name=".BookTicketsActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOKTICKETSACTIVITY" />

                <!--<category android:name="android.intent.category.LAUNCHER" /> -->
            </intent-filter>
        </activity>

        <activity
            android:name=".SuccessMessage"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.SUCCESSMESSAGE" />

                <!--<category android:name="android.intent.category.LAUNCHER" /> -->
            </intent-filter>
        </activity>

        <activity
            android:name=".TestActivity"
